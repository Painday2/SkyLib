<dynamic_object>
    <diesel orientation_object="rp_root" materials="units/pd2_mod_zombies/architecture/zom_dah_tiles/zom_dah" />
    <bodies>
        <body enabled="true" template="static" name="static_body" >
            <object name="rp_root" />
            <object name="co_culling" />
			<object name="g_roof" collision_type="mesh_mopp" padding="-2.5" />
			<object name="g_ceiling" collision_type="mesh_mopp" padding="-2.5" />
			<object name="g_trim" collision_type="mesh_mopp" padding="-2.5" />
			<object name="g_ext" collision_type="mesh_mopp" padding="-2.5" />
        </body>
    </bodies>
	<decal_surfaces default_material="plaster">
		<decal_mesh name="g_ceiling" enabled="true" material="sheet_metal" />
		<decal_mesh name="g_trim" enabled="true" material="sheet_metal" />
		<decal_mesh name="g_roof" enabled="true" material="concrete" />
		<decal_mesh name="g_ext" enabled="true" material="concrete" />
	</decal_surfaces>
	<graphics>
		<graphic_group name="graphics" culling_object="co_culling" enabled="true">
			<object name="g_roof" enabled="true" shadow_caster="true"/>
			<object name="g_ceiling" enabled="true" shadow_caster="true"/>
			<object name="g_trim" enabled="true" shadow_caster="true"/>
			<object name="g_ext" enabled="true" shadow_caster="true"/>
		</graphic_group>
	</graphics>
</dynamic_object>
